using UnityEngine;
using System.Collections;

public class PlayQuitController : MonoBehaviour {

	// Use this for initialization
	void Start () {
        
	
	}
	
	// Update is called once per frame
	void Update () {
	    if (Input.GetKeyDown(KeyCode.Q))
        {
            quit();
        }
	}

    void start()
    {
        
    }
    void quit()
    {
        Application.Quit();
    }
}
